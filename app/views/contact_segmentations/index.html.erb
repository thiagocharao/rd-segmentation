<p id="notice"><%= notice %></p>

<h1>Contact Segmentations</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_segmentations.each do |contact_segmentation| %>
      <tr>
        <td><%= contact_segmentation.description %></td>
        <td><%= link_to 'Show', contact_segmentation %></td>
        <td><%= link_to 'Edit', edit_contact_segmentation_path(contact_segmentation) %></td>
        <td><%= link_to 'Destroy', contact_segmentation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact Segmentation', new_contact_segmentation_path %>
